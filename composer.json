        "x.509",
        "certificate",
        "attribute certificate",
        "ac",
        "certification request",
        "csr"
    ],
    "authors": [
        {
            "name": "Joni Eskelinen",
            "email": "jonieske@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Artem Vasilev",
            "email": "kern.usr@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-gmp": "*",
        "sop/asn1": "^4.0.0",
        "webmasterskaya/x501": "dev-master",
        "webmasterskaya/crypto-types": "dev-master",
        "webmasterskaya/crypto-bridge": "dev-master",
        "sop/crypto-encoding": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Webmasterskaya\\X509\\": "lib/X509/"
        }
    }
}